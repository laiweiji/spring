<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>classpath:application-06.xml</param-value>
    </context-param>
    <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>

    <servlet>
        <servlet-name>funServlet</servlet-name>
        <servlet-class>cn.study.servlet.funServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>BooknewServlet</servlet-name>
        <servlet-class>SprMy.servlet.BooknewServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>BooknewServlet</servlet-name>
        <url-pattern>/BooknewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>funServlet</servlet-name>
        <url-pattern>/funServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ooServlet</servlet-name>
        <servlet-class>cn.study.servlet.ooServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ooServlet</servlet-name>
        <url-pattern>/ooServlet</url-pattern>
    </servlet-mapping>



</web-app>
